BASE=/projects/llm-basics/camel/tools
LAB=$BASE/lab
FF=$BASE/solution/fastforward

STEP=`head $BASE/solution/scripts/step`
NEXT=step$(($STEP))

echo "fast forward..."
rm -r $LAB/*
cp -r $FF/$NEXT/* .
echo "step $(($STEP)) done."
echo "step $(($STEP+1)) ready"
echo "$(($STEP+1))" > $BASE/solution/scripts/step